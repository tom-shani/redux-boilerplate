{
  "name": "redux-boilerplate",
  "version": "1.2.0",
  "description": "Basic Hot-Loading, Universal Redux boilerplate",
  "main": "bin/server.js",
  "devDependencies": {
    "eslint": "2.8.0",
    "eslint-config-pomo": "1.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-import": "1.5.0",
    "eslint-plugin-react": "5.0.1"
  },
  "dependencies": {
    "babel-core": "6.7.7",
    "babel-eslint": "6.0.3",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-typecheck": "3.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "better-npm-run": "0.0.8",
    "body-parser": "^1.15.0",
    "bootstrap-loader": "1.0.10",
    "bootstrap-sass": "3.3.6",
    "clean-webpack-plugin": "0.1.8",
    "color": "0.11.1",
    "compression": "1.6.1",
    "concurrently": "2.0.0",
    "cookie-session": "^2.0.0-alpha.1",
    "cors": "^2.7.1",
    "css-loader": "0.23.1",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "fs": "0.0.2",
    "history": "2.1.0",
    "http-proxy": "1.13.2",
    "immutable": "3.8.1",
    "json-loader": "0.5.4",
    "lodash": "4.11.1",
    "mongoose": "^4.4.13",
    "node-sass": "3.5.3",
    "piping": "0.3.0",
    "postcss-loader": "0.8.2",
    "pretty-error": "2.0.0",
    "react": "15.0.1",
    "react-bootstrap": "0.29.2",
    "react-dom": "15.0.1",
    "react-helmet": "3.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.3.0",
    "react-router-redux": "4.0.2",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.2.3",
    "redux": "3.5.2",
    "redux-devtools": "3.2.0",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-thunk": "2.0.1",
    "resolve-url-loader": "1.4.3",
    "sass-loader": "3.2.0",
    "scroll-behavior": "^0.4.0",
    "serialize-javascript": "1.2.0",
    "serve-favicon": "2.3.0",
    "strip-loader": "0.1.2",
    "style-loader": "0.13.1",
    "superagent": "1.8.3",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0",
    "webpack-isomorphic-tools": "2.2.48",
    "webpack-merge": "0.12.0"
  },
  "scripts": {
    "build": "better-npm-run build",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "lint": "./node_modules/eslint/bin/eslint.js --config .eslintrc src api || true",
    "start": "concurrently --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "watch-client": "better-npm-run watch-client"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --progress --verbose --colors --display-error-details --config webpack/config.prod.babel.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node ./bin/dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/pomo-io/pomo.design.git"
  },
  "author": "Tom Adam Shani <tom@tomshani.com>",
  "license": "ISC",
  "homepage": "https://bitbucket.org/pomo-io/pomo.design#readme"
}
