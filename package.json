{
  "name": "redux-boilerplate",
  "version": "1.0.0",
  "description": "Basic Hot-Loading, Universal Redux boilerplate",
  "main": "bin/server.js",
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta9",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-typecheck": "^3.6.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-node5": "^1.1.1",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "better-npm-run": "^0.0.7",
    "bootstrap-loader": "^1.0.3",
    "bootstrap-sass": "^3.3.5",
    "clean-webpack-plugin": "^0.1.6",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.1.0",
    "eslint-config-pomo": "^1.0.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-import": "^1.0.0-beta.0",
    "eslint-plugin-react": "^4.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "piping": "^0.3.0",
    "postcss-loader": "^0.8.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.2",
    "strip-loader": "^0.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-isomorphic-tools": "^2.2.24",
    "webpack-merge": "^0.7.3"
  },
  "dependencies": {
    "color": "^0.11.1",
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "fs": "0.0.2",
    "history": "^2.0.0",
    "hoist-non-react-statics": "^1.0.3",
    "http-proxy": "^1.12.0",
    "immutable": "^3.7.6",
    "lodash": "^4.0.0",
    "moment": "^2.11.1",
    "pretty-error": "^2.0.0",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.6",
    "react-helmet": "^2.2.0",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc6",
    "react-router-redux": "^3.0.0",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.3",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "superagent": "^1.6.1"
  },
  "scripts": {
    "build": "webpack --progress --verbose --colors --display-error-details --config webpack/config.prod.babel.js",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" ",
    "start": "concurrent --kill-others \"npm run start-prod\" ",
    "start-dev": "better-npm-run start-dev",
    "start-prod": "better-npm-run start-prod",
    "lint": "./node_modules/eslint/bin/eslint.js -c .eslintrc src || true",
    "watch-client": "better-npm-run watch-client"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/pomo-io/pomo.design.git"
  },
  "author": "Tom Adam Shani <tom@tomshani.com>",
  "license": "ISC",
  "homepage": "https://bitbucket.org/pomo-io/pomo.design#readme"
}
